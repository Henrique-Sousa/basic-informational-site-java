<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Servlet</servlet-name>
  	<display-name>Servlet</display-name>
  	<description></description>
  	<servlet-class>br.com.henriquesousa.Servlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>IndexServlet</servlet-name>
  	<display-name>IndexServlet</display-name>
  	<description></description>
  	<servlet-class>br.com.henriquesousa.IndexServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AboutServlet</servlet-name>
  	<display-name>AboutServlet</display-name>
  	<description></description>
  	<servlet-class>br.com.henriquesousa.basic_informational_site.servlets.AboutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ContactMeServlet</servlet-name>
  	<display-name>ContactMeServlet</display-name>
  	<description></description>
  	<servlet-class>br.com.henriquesousa.basic_informational_site.servlets.ContactMeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Servlet</servlet-name>
  	<url-pattern>/Servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>IndexServlet</servlet-name>
  	<url-pattern>/IndexServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AboutServlet</servlet-name>
  	<url-pattern>/AboutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ContactMeServlet</servlet-name>
  	<url-pattern>/ContactMeServlet</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/404.jsp</location>
  </error-page>
</web-app>